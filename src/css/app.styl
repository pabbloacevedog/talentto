.modal-open .q-layout--standard, .modal-open .q-layout {
	-webkit-filter: blur(6px);
	-moz-filter: blur(2px);
	-o-filter: blur(2px);
	-ms-filter: blur(2px);
	filter: blur(5px);
}

body {
	font-family: 'monserrat_regular';
}

.q-field__messages > div {
	color: #ff8037;
}

.q-chip {
	font-size: 13px !important;
	// padding: 16px;
}

.btn_add {
	position: absolute;
	padding: 17px !important;
	right: 14px;
	color: #fff;
	background: $primary !important;
}

.icon_add {
	font-size: 3em !important;
	border-radius: 25px !important;
}

.q-tooltip {
	font-size: 16px !important;
	background: $accent !important;
	max-width: 600px !important;
}

::-webkit-scrollbar {
	height: 12px;
	width: 14px;
	background: transparent;
	z-index: 12;
	overflow: visible;
	display: none;
}

::-webkit-scrollbar-thumb {
	width: 10px;
	background-color: $accent;
	border-radius: 10px;
	z-index: 12;
	border: 4px solid rgba(0, 0, 0, 0);
	background-clip: padding-box;
	transition: background-color 0.32s ease-in-out;
	margin: 4px;
	min-height: 32px;
	min-width: 32px;
}

::-webkit-scrollbar-thumb:hover {
	background: $accent;
}

.q-table th, .q-table td {
	padding: 0px 0px;
	background-color: inherit;
}

.div_fondo {
	height: 111vh !important;
	background-size: cover;
}

.ver_mas {
	background: linear-gradient(to right, #f12711, #f5af19);
	padding: 16px 23px !important;
}

.q-drawer {
	background: $primary !important;
	backdrop-filter: blur(8.5px) !important;
	-webkit-backdrop-filter: blur(8.5px) !important;
}

.tabla {
	background: $primary !important;
	color: $accent !important;
	border-radius: 15px;
}

.tabla_mobile {
	background: $primary !important;
	color: $accent !important;
	border-radius: 15px;
}

.buscar_mobile .q-field__control, .q-field--dense .q-field__marginal {
	max-height: 30px !important;
	padding-bottom: 3px !important;
	color: $text;
}

.buscar_mobile .q-field--standout .q-field__control {
	padding: 0px 3px;
	background: rgba(0, 0, 0, 0.05);
	border-radius: 4px;
	color: $text;
	transition: box-shadow 0.36s cubic-bezier(0.4, 0, 0.2, 1), background-color 0.36s cubic-bezier(0.4, 0, 0.2, 1);
}

.buscar_mobile .q-field__inner .q-field__control .q-field__control-container .q-placeholder {
	color: #fff;
}

.card_tabla_mobile {
	background: $primary !important;
	box-shadow: 0 0px 0px rgb(0 0 0 / 20%), 0 0px 0px rgb(0 0 0 / 14%), 0 0px 0px 0px rgb(0 0 0 / 12%);
}

.card_tabla_mobile_selected {
	background: $secondary !important;
	box-shadow: 0 0px 0px rgb(0 0 0 / 20%), 0 0px 0px rgb(0 0 0 / 14%), 0 0px 0px 0px rgb(0 0 0 / 12%);
}

.card_tabla_mobile .q-list .q-item .q-item__section .q-item__label--caption {
	color: $text !important;
	max-width: 200px;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}

.card_tabla_mobile_selected .q-list .q-item .q-item__section .q-item__label--caption {
	color: $text !important;
	max-width: 200px;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}

.q-table th {
	color: $accent;
	font-size: 16px !important;
	background-color: inherit;
	font-family: comfortaa_b;
	max-width: 200px !important;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}

.q-table td {
	color: $text;
	font-size: 14px !important;
	font-family: comfortaa_l;
	max-width: 200px !important;
	background-color: inherit;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}

.b_activo {
	color: $text;
	font-size: 14px !important;
	font-family: comfortaa_l;
}

.b_eliminar {
	color: $text;
	font-size: 16px !important;
	font-family: comfortaa_l;
}

.b_link {
	color: $accent;
	font-size: 14px !important;
	font-family: comfortaa_l;
}

.q-table__title {
	font-size: 23px;
	letter-spacing: 0.005em;
	font-weight: 400;
	padding-left: 10px;
	font-family: comfortaa_b;
}

.modales {
	background: $secondary !important;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	border-radius: 15px !important;
}

.cancelar {
	margin-right: 20px;
	color: $text;
	background: $primary !important;
}

.n_eliminar {
	color: $accent;
	font-size: 16px !important;
	font-family: comfortaa_b;
}

.grid-style-transition {
	transition: transform 0.28s, background-color 0.28s;
}

.activo {
	background: #00933b !important;
}

.inactivo {
	background: #cd0000 !important;
}
.input-reg .q-field__inner .q-field__control .q-field__control-container textarea {
	color: #9d9d9d !important;
}
.input-reg {
	padding-bottom: 30px !important;
}
.q-field__label {
	color: #fff !important;
}
.titulo_crear {
	color: #f84814 !important;
	font-family: comfortaa_b;
}
.q-checkbox__inner {
	color: rgb(138 53 31);
}
.input-reg .q-field__inner .q-field__control .q-field__control-container input {
	color: #9d9d9d !important;
}
.q-carousel .q-carousel__thumbnail {
	margin: 7px;
	height: 55px;
	width: 55px;
	display: inline-block;
	cursor: pointer;
	border: none;
	border-radius: 4px;
	vertical-align: middle;
	opacity: 0.7;
	transition: opacity 0.3s;
}

.q-carousel__thumbnail {
	align-items: center;
	border-radius: 50% !important;
	padding: 2px;
	background-image: linear-gradient(
		45deg,
		#ffa95f 5%,
		#f99c4a 15%,
		#f47838 30%,
		#e75157 45%,
		#d92d7a 70%,
		#cc2a92 80%,
		#c32e92 95%
	);
}

.q-carousel__thumbnail--active {
	background-image: linear-gradient(45deg, #31bf03 5%, #00f59b 95%);
}

.q-carousel__control {
	color: #fff0 !important;
}
.q-carousel__navigation--bottom {
	top: 1px !important;
}
.titulo-areas {
	font-size: 3em;
	font-family: 'comfortaa_b';
}
.titulo-historia {
	font-size: 3em;
	font-family: 'comfortaa_b';
}
.titulo-snosotros {
	font-size: 2.8em;
	font-family: 'comfortaa_b';
}
.q-carousel__navigation-inner {
	height: max-content !important;
	padding-top: 20px !important;
}
